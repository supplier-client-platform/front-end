{
  "project": {
    "version": "1.0.0-beta.15",
    "name": "frontend"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": "assets",
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "./assets/css/bootswatch.css",
        "./assets/css/animate.css",
        "./assets/css/main.css",
        "./assets/theme/css/bootstrap-reset.css",
        "./assets/theme/css/style.css",
        "./assets/theme/css/style-responsive.css",
        "./assets/font-awesome/css/font-awesome.css",
        "../node_modules/bootstrap-datepicker/dist/css/bootstrap-datepicker.css",
        "../node_modules/bootstrap-timepicker/css/bootstrap-timepicker.css",
        "../node_modules/ng2-toasty/style.css",
        "../node_modules/ng2-toasty/style-bootstrap.css",
        "styles.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.js",
        "../node_modules/bootstrap-datepicker/dist/js/bootstrap-datepicker.js",
        "../node_modules/bootstrap-timepicker/js/bootstrap-timepicker.js",
        "../node_modules/chart.js/dist/Chart.js",
        "../node_modules/jspdf/dist/jspdf.min.js",
        "../node_modules/jspdf-autotable/dist/jspdf.plugin.autotable.js",
        "../node_modules/moment/min/moment.min.js"
      ],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false
  }
}
